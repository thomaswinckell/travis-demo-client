{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "strictNullChecks": true,
    "target": "es6",
    "module": "es6",
    "moduleResolution": "node",
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true
  }
}